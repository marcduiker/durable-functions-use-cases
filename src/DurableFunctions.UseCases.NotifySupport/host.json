{
    "version": "2.0",
    "extensions": {
        "durableTask": {
            "hubName": "%TaskHubName%",
            "storageProvider": {
              "maxQueuePollingInterval": "00:00:05"
            }
        }
    },
    "logging": {
        "applicationInsights": {
            "samplingExcludedTypes": "Request",
            "samplingSettings": {
                "isEnabled": true
            }
        }
    }
}